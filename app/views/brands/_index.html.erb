<div class="col-md-8 col-md-offset-2">
<div class="card card-outline style-accent">
  <div class="card-head">
  	<header>Brands</header>
  </div>
<div class="white no-pad">
<div class="row">
    <% brands.each do |b| %>
        <%= link_to ({action: "index", controller: "campaigns", brand_id: b.id, remote: true}) do %>
            <div class="card col-md-5">
              <%= b.name %>
            </div>
        <% end %>
    <% end %>
</div>
</div>
</div>
</div>