<!-- WEB -->
Web / Organic Data
<div class="row">
  <!-- Likes -->
  <div class="col-md-3 col-sm-6">
    <div class="card">
      <div class="card-body no-padding">
        <div class="alert alert-callout alert-info no-margin">
          <strong class="text-xl"><%= @impressions.count %></strong><br/>
          <span class="opacity-50">Impressions</span>
        </div>
      </div><!--end .card-body -->
    </div><!--end .card -->
  </div><!--end .col -->

  <!-- Comments -->
  <div class="col-md-3 col-sm-6">
    <div class="card">
      <div class="card-body no-padding">
        <div class="alert alert-callout alert-warning no-margin">
          <strong class="text-xl"><%= @impressions.inject(0){|sum,x| sum + x.view_time } %></strong><br/>
          <span class="opacity-50">View (sec)</span>
        </div>
      </div><!--end .card-body -->
    </div><!--end .card -->
  </div><!--end .col -->

  <!-- Likes per Impression -->
  <div class="col-md-3 col-sm-6">
    <div class="card">
      <div class="card-body no-padding">
        <div class="alert alert-callout alert-danger no-margin">
          <strong class="text-xl"><%= @impressions.inject(0){|sum,x| sum + x.hover_time } %></strong><br/>
          <span class="opacity-50">Hover (sec)</span>

        </div>
      </div><!--end .card-body -->
    </div><!--end .card -->
  </div><!--end .col -->

  <!-- Cost per Impression -->
  <div class="col-md-3 col-sm-6">
    <div class="card">
      <div class="card-body no-padding">
        <div class="alert alert-callout alert-success no-margin">

          <strong class="text-xl"><%= ((@impressions.inject(0){|sum,x| sum + x.view_time }) / @impressions.count) %></strong><br/>
          <span class="opacity-50">View (sec) per Impression</span>
        </div>
      </div><!--end .card-body -->
    </div><!--end .card -->
  </div><!--end .col -->
</div><!--end .row -->
<!-- WEB END -->
