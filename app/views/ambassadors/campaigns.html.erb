<!DOCTYPE html>
<html lang="en">
    <head>        
        <!-- META SECTION -->
        <title>Sway Up</title>            
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        
        <link rel="icon" href="favicon.ico" type="image/x-icon" />
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">

        <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  		<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  		<%= csrf_meta_tags %>      
  	    <link rel="stylesheet" href="assets/theme-default.css.scss">
  		<style type="text/css">
		  .x-navigation.x-navigation-minimized {
		  min-height: 670px!important;
		}
  		</style>
  		<script type="text/javascript">
  		    $( document ).ready(function(){
  			   $(".xn-icon-button").on('click', function(){
  			   $(".page-sidebar .x-navigation").toggleClass("x-navigation-minimized");
  			   $(".page-container").toggleClass("page-navigation-toggled page-container-wide");
  				});
            });
  		</script>     
    </head>
    <body>
        <!-- START PAGE CONTAINER -->
        <div class="page-container">
            
            <!-- START PAGE SIDEBAR -->
            <%= render 'layouts/sidebar.html.erb'%>
            <!-- END PAGE SIDEBAR -->
            
            <!-- PAGE CONTENT -->
            <div class="page-content">
            <%= render 'layouts/topbar.html.erb'%>
                <!-- START BREADCRUMB -->
                <ul class="breadcrumb">
                    <li><a href="#">Home</a></li>                    
                    <li class="active">View Campaigns</li>
                </ul>
                <!-- END BREADCRUMB -->                       
                
                <!-- PAGE CONTENT WRAPPER -->
                <div class="page-content-wrap">  
                    <div class="alert alert-warning">
                        <strong>Warning!</strong> Indicates a warning that might need attention.
                    </div>
                    <div class="row">
                        <div class="col-lg-3 campaigns">
                            Campaign 1
                        </div>
                        <div class="col-lg-3 campaigns">
                            Campaign 2
                        </div>
                        <div class="col-lg-3 campaigns">
                            Campaign 3
                        </div>
                        <div class="col-lg-3 campaigns">
                            Campaign 4
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-lg-3 campaigns">
                            Campaign 5
                        </div>
                        <div class="col-lg-3 campaigns">
                            Campaign 6
                        </div>
                        <div class="col-lg-3 campaigns">
                            Campaign 7
                        </div>
                        <div class="col-lg-3 campaigns">
                            Campaign 8
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-lg-3 campaigns">
                            Campaign 9
                        </div>
                        <div class="col-lg-3 campaigns">
                            Campaign 10
                        </div>
                        <div class="col-lg-3 campaigns">
                            Campaign 11
                        </div>
                        <div class="col-lg-3 campaigns">
                            Campaign 12
                        </div>
                    </div>
               
                </div>
                <!-- END PAGE CONTENT WRAPPER -->                                
            </div>            
            <!-- END PAGE CONTENT -->
        </div>
        <!-- END PAGE CONTAINER -->
        <!-- START PRELOADS -->
        <audio id="audio-alert" src="audio/alert.mp3" preload="auto"></audio>
        <audio id="audio-fail" src="audio/fail.mp3" preload="auto"></audio>
        <!-- END PRELOADS -->                  
        
    </body>
</html>






