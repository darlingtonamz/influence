<script type="text/javascript">
            $( document ).ready(function(){
               $(".x-navigation").on('click', function(){
                if($("li").hasClass()){
                    $("li").removeClass();
                }
                $(this).addClass("active");
                //$(".page-container").toggleClass("page-navigation-toggled page-container-wide");

                });
            });
        </script> 
<div class="page-sidebar">
                <!-- START X-NAVIGATION -->
                <% ig_user = current_user.socials.where(provider: "instagram").first %>
                <ul class="x-navigation">
                    <li class="xn-logo">
                        <a href="index.html">Sway Up</a>
                        <a href="#" class="x-navigation-control"></a>
                    </li>
                    <li class="xn-profile">
                        <a href="#" class="profile-mini">
                            <% if current_user.socials.where(provider: "instagram").blank? %>
                                <%= image_tag('blank-user.png') %>
                            <% else %>
                                <%= image_tag(ig_user.photo, alt: "Your profile photo") %> 
                            <% end %>
                            
                        </a>
                        <div class="profile">
                            <div class="profile-image">
                            <% if current_user.socials.where(provider: "instagram").blank? %>
                                <%= image_tag('blank-user.png') %>
                            <% else %>
                                <%= image_tag(current_user.socials.where(provider: "instagram").first.photo) %>
                            <% end %>
                                
                            </div>
                            <div class="profile-data">
                            <div class="profile-data-name"><%= current_user.email %></div>
                            <% if !current_user.socials.where(provider: "instagram").blank? %>
                                <div class="profile-data-title"><%= ig_user.bio %></div>
                            <% end %>
                            </div>
                            <div class="profile-controls">
                                <a href="pages-profile.html" class="profile-control-left"><span class="fa fa-info"></span></a>
                            </div>
                        </div>                                                                        
                    </li>
                    <li class="active">
                        <%= link_to ambassadors_path do%>
                            <span class="fa fa-desktop"></span> <span class="xn-text">Dashboard</span>
                        <% end %>
                    </li>                    
                    
                    <li>
                        <%= link_to ambassadors_campaigns_path do%>
                            <span class="fa fa-hashtag"></span> <span class="xn-text">View Campaigns Invites</span>
                        <% end %>
                    </li>
                    <li>
                        <%= link_to ongoing_list_path, remote: true do%>
                            <span class="fa fa-hashtag"></span> <span class="xn-text">Ongoing Campaigns</span>
                        <% end %>
                    </li>
                    <li>
                        <%= link_to '#', remote: true do%>
                            <span class="fa fa-hashtag"></span> <span class="xn-text">View Campaigns Invites</span>
                        <% end %>
                    </li>
                </ul>
                <!-- END X-NAVIGATION -->
            </div>